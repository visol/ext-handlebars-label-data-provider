    "description": "Auto inject labels from the sinso/translationapi",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "Visol\\HandlebarsLabelDataProvider\\": "Classes/"
        }
    },
    "require": {
        "php": "~7.4.0 || ~8.0.0 || ~8.1.0",
